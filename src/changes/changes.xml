<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://maven.apache.org/changes/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/changes/1.0.0 http://maven.apache.org/xsd/changes-1.0.0.xsd">

  <properties>
    <title>Changes</title>
    <author email="stefan.graff@gmx.at">Stefan Graff</author>
  </properties>
  <body>
    <release version="3.0.0-SNAPSHOT" description="Jakarta EE 9+, JDK 11, JPMS">
      <action type="update" dev="ste-gr">Bump &quot;maven-clean-plugin&quot; from 3.2.0 to 3.3.2</action>
      <action type="update" dev="ste-gr">Bump &quot;checkstyle&quot; from 9.3 to 10.13.0</action>
      <action type="update" dev="ste-gr">Bump &quot;maven-checkstyle-plugin&quot; from 3.1.2 to 3.3.1</action>
      <action type="fix" dev="ste-gr">Add missing plugin (assembly)</action>
      <action type="fix" dev="ste-gr">Add missing plugin (antrun)</action>
      <action type="add" dev="ste-gr">Add &quot;versions-maven-plugin&quot; with reports</action>
      <action type="update" dev="ste-gr">Set version to 3.0.0-SNAPSHOT</action>
    </release>
    <release version="2.6" date="2022-07-19" description="">
      <action type="update" dev="ste-gr">Bump &quot;tlddoc&quot; from 1.4-SNAPSHOT to 1.4</action>
      <action type="fix" dev="ste-gr">Unescape HTML in tag-description-list when parse-html is on.</action>
      <action type="fix" dev="ste-gr">A few embellishments to the Tag-Reference-Report</action>
      <action type="fix" dev="ste-gr">Typo in properties</action>
      <action type="update" dev="ste-gr">Use varargs for &quot;MessageFormat.format&quot;</action>
      <action type="fix" dev="ste-gr">Lint markdown-files</action>
      <action type="fix" dev="ste-gr">Some Report-corrections</action>
      <action type="update" dev="ste-gr">JDK8: Use For-Each-Loops</action>
      <action type="update" dev="ste-gr">Bump &quot;jdom&quot; (new &quot;jdom2&quot;) from 2.0.2 to 2.0.6.1</action>
      <action type="update" dev="ste-gr">Bump &quot;plexus-archiver&quot; from 4.2.7 to 4.4.0</action>
      <action type="update" dev="ste-gr">Bump &quot;spotbugs-maven-plugin&quot; from 4.7.0.0 to 4.7.1.0</action>
      <action type="update" dev="ste-gr">Bump &quot;maven-surefire-plugin&quot; from 3.0.0-M6 to 3.0.0-M7</action>
      <action type="update" dev="ste-gr">Bump &quot;maven-release-plugin&quot; from 3.0.0-M5 to 3.0.0-M6</action>
      <action type="update" dev="ste-gr">Bump &quot;maven-pmd-plugin&quot; from 3.16.0 to 3.17.0</action>
      <action type="update" dev="ste-gr">Bump &quot;maven-enforcer-plugin&quot; from 3.0.0 to 3.1.0</action>
      <action type="update" dev="ste-gr">Bump &quot;maven-plugins&quot; from 3.8.5 to 3.8.6</action>
      <action type="fix" dev="ste-gr">Set reporting-output-encoding to UTF-8</action>
      <action type="update" dev="ste-gr">Upgrade to &quot;doxia.xhtml5&quot;</action>
      <action type="fix" dev="ste-gr">Correct FAQs</action>
      <action type="update" dev="ste-gr">Bump &quot;maven-assembly-plugin&quot; from 3.3.0 to 3.4.1</action>
      <action type="fix" dev="ste-gr">Correct some SpotBugs</action>
      <action type="fix" dev="ste-gr">Remove unused resources</action>
      <action type="add" dev="ste-gr">Add &quot;compareTo&quot;, &quot;equals&quot; and &quot;hashCode&quot; to checker-classes</action>
      <action type="update" dev="ste-gr">Change distribution to central-repo</action>
      <action type="add" dev="ste-gr">Add new profile &quot;release&quot; to deploy to central-repo</action>
      <action type="update" dev="ste-gr">Update copyright</action>
      <action type="add" dev="ste-gr">Update configs for &quot;license-maven-plugin&quot; and add &quot;build-helper-maven-plugin&quot; to get current year</action>
      <action type="add" dev="ste-gr">Add new profile &quot;assembly&quot; to generate assemblies</action>
      <action type="update" dev="ste-gr">Change group-id from &quot;net.sourceforge.maven-taglib&quot; to &quot;io.github.weblegacy&quot;</action>
      <action type="update" dev="ste-gr">Use new &quot;io.github.weblegacy:tlddoc&quot; dependency</action>
      <action type="fix" dev="ste-gr">Remove all compiler-warnings</action>
      <action type="fix" dev="ste-gr">Remove unnecessary setters in TagreferenceMojo</action>
      <action type="fix" dev="ste-gr">Remove PMD-Warnings and exclude generated classes form PMD-report</action>
      <action type="add" dev="ste-gr">Use parameter locale for generating localized reports</action>
      <action type="fix" dev="ste-gr">Add/remove dependencies because Dependency Analysis</action>
      <action type="update" dev="ste-gr">Upgrade from commons-lang to commons-lang3</action>
      <action type="update" dev="ste-gr">Update site.xml</action>
      <action type="fix" dev="ste-gr">Remove double used template info at tld1_1-tld1_2.xsl</action>
      <action type="fix" dev="ste-gr">Small POM-changes: license-name, remove property, configurations for javadoc- and compiler-plugin</action>
      <action type="add" dev="ste-gr">Add plugins/reports: checkstyle, dependency, gpg, pmd, release, scm-publish, spotbugs</action>
      <action type="update" dev="ste-gr">Set new home-url</action>
      <action type="update" dev="ste-gr">Bump tlddoc from 1.3 to 1.4-SNAPSHOT</action>
      <action type="update" dev="ste-gr">Bump maven-reporting-impl from 3.1.0 to 4.0.0-M1</action>
      <action type="update" dev="ste-gr">Bump maven-project-info-reports-plugin from 3.2.2 to 3.3.0</action>
      <action type="update" dev="ste-gr">Bump license-maven-plugin from 4.1 to 4.2.rc3</action>
      <action type="update" dev="ste-gr">Use current web-jsptaglibrary*.dtd from jboss-project</action>
      <action type="update" dev="ste-gr">Adaption for JDK 9+</action>
      <action type="fix" dev="ste-gr">Add links for JavaDoc</action>
      <action type="fix" dev="ste-gr">Workaround MJAVADOC-700</action>
      <action type="fix" dev="ste-gr">Generate JavaDoc/JXR-Report without duplicate execution</action>
      <action type="fix" dev="ste-gr">Add/Correct JavaDocs to remove JavaDoc-Generation-Errors and Warnings</action>
      <action type="update" dev="ste-gr">Bump maven-site-plugin from 3.11.0 to 3.12.0</action>
      <action type="update" dev="ste-gr">Bump maven-javadoc-plugin from 3.3.2 to 3.4.0</action>
      <action type="fix" dev="ste-gr">Remove closeReport in TagreferenceMojo</action>
      <action type="update" dev="ste-gr">Some Code-optimization in TagreferenceRenderer</action>
    </release>
    <release version="2.5" date="2014-08-15" description="">
      <action type="update" dev="fgiust">Updated plugin to use maven 3 dependencies/APIs.</action>
      <action type="update" dev="fgiust">Changed licensing from "Artistic license" to MIT.</action>
      <action type="add" dev="fgiust" due-to="Matthias Nüßler">Provide help mojo. Fixes MAVENTAGLIB-18</action>
    </release>
    <release version="2.4" date="2010-01-31" description="">
    </release>
    <release version="2.3.1" date="2007-09-17" description="">
      <action type="add" dev="fgiust" due-to="Bernd Bohmann">include the short name in the local anchor.</action>
      <action type="add" dev="fgiust" due-to="Bernd Bohmann">allow parsing of the html in the description of tld info, tag
        description, attribute description and variable description.
      </action>
    </release>
    <release version="2.3" date="2006-08-20" description="">
      <action type="add" dev="fgiust">Support SimpleTag based tags in taglib validation.</action>
      <action type="fix" dev="fgiust">Support for tag files was broken in taglibdoc Mojo</action>
      <action type="fix" dev="fgiust" issue="1483561">Tag reference renderer doesn't require java 1.5 anymore</action>
      <action type="add" dev="fgiust" issue="1529660" due-to="Jonni Gani">Array type support in ValidateRenderer
      </action>
      <action type="add" dev="fgiust">Added the tldgenerate goal: generates a tld file from a directory containing jsp
        2.0 tag files.
      </action>
      <action type="update" dev="fgiust">The jar generated by the taglibdocjar goal is now in the main bulid output
        directory, instead that in the site/tlddoc folder. New configuration parameter tlddocJar for overriding this
        default.
      </action>
    </release>
    <release version="2.2.1" date="2006-02-12" description="">
      <action type="fix" dev="fgiust">Text in CDATA sections for tag/attribute description was not properly shown in tag
        reference page
      </action>
      <action type="fix" dev="fgiust">Tag reference report had a wrong title</action>
    </release>
    <release version="2.2" date="2006-01-21" description="">
      <action type="update" dev="fgiust">Update taglibrarydoc to version 1.3 (adds support for jsp 2.1 tlds)</action>
      <action type="fix" dev="fgiust" due-to="Bernd Bohmann">A working anchor in the TagreferenceRenderer</action>
      <action type="fix" dev="fgiust" due-to="Bernd Bohmann">Classloader issues in validation report</action>
      <action type="add" dev="fgiust" due-to="Bernd Bohmann">new taglibdocjar goal</action>
    </release>
    <release version="2.1" date="2006-01-01" description="">
      <action type="add" dev="fgiust" due-to="Bernd Bohmann">Add support for tld's in subdirectories.</action>
      <action type="add" dev="fgiust">Handle tag files in taglibdoc goal.</action>
      <action type="fix" dev="fgiust">Fix taglibdoc output directory in multiproject builds (tlddoc files were
        incorrectly generated in the root project).
      </action>
    </release>
    <release version="2.0" date="2005-09-30" description="Initial release for m2">
      <action type="add" dev="fgiust">First release of the maven taglib plugin for m2.</action>
    </release>
  </body>
</document>